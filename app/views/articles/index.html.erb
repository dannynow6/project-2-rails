<header id="main-header">
    <h1 id="project-title">
        Project 2
    </h1>
    <br/>
</header>

<h2 id="page-title">
    Articles 
</h2>

<div id="main-section">
    <div id="article-display">
        <ul id="article-list">
            <% @articles.each do |article| %>
                <li>
                    <a href="/articles/<%= article.id %>">
                        <%= article.title %>
                    </a>
                </li>
                <br/>
            <% end %> 
        </ul>
    </div>
</div>
